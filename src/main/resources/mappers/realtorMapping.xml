<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mink.springtest.mvc.repository.RealtorRepository">
    <insert id="insertRealtor" parameterType="com.mink.springtest.mvc.domain.Realtor"
            useGeneratedKeys="true" keyProperty="id">
        INSERT INTO
        `realtor`
        (
            `office`
            ,`phone_number`
            ,`address`
            ,`grade`
        )
        VALUES
            (
              #{office}
            ,#{phoneNumber}
            ,#{address}
            ,#{grade}
            )
    </insert>

</mapper>