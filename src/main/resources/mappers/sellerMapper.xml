<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mink.springtest.mvc.repository.SellerRepository">
    <select id="selectSeller"
            resultType="com.mink.springtest.mvc.domain.Seller">
        SELECT
            `id`
            ,`nickname`
            ,`temperature`
            ,`profile_image`
            ,`created_at`
            ,`updated_at`
        FROM
            `seller`
    </select>
</mapper>