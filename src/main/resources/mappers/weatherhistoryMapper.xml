<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mink.springtest.thymeleaf.test04.repository.WeatherhistoryRepository" >
    <select id="selectWeatherhistoryList" resultType="com.mink.springtest.thymeleaf.test04.domain.Weatherhistory">
        SELECT
            `id`
             , `weather`
             , `date`
             , `temperatures`
             , `precipitation`
             , `micro_dust`
             , `wind_speed`
             , `created_at`
             , `updated_at`

        FROM
            `weatherhistory`
    </select>
    <insert id="insertWeatherhistoryList" parameterType="map">
        INSERT INTO
            `weatherhistory`
        (
          `weather`
        , `date`
        , `temperatures`
        , `precipitation`
        , `micro_dust`
        , `wind_speed`
        )
            VALUE
            (
            #{weather}
            , #{date}
            , #{temperatures}
            , #{precipitation}
            , #{microDust}
            , #{windSpeed}
            )
    </insert>

    <insert id="insertWeatherhistoryByObject" parameterType="com.mink.springtest.thymeleaf.test04.domain.Weatherhistory">
        INSERT INTO
            `weatherhistory`
        (
          `weather`
        , `date`
        , `temperatures`
        , `precipitation`
        , `micro_dust`
        , `wind_speed`
        )
            VALUE
            (
            #{weather}
            , #{date}
            , #{temperatures}
            , #{precipitation}
            , #{microDust}
            , #{windSpeed}
            )
    </insert>


</mapper>